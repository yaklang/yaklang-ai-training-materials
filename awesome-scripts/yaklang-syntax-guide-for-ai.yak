        }
    }
    
    // è¾“å‡ºç»“æœ
    yakit.StatusCard("ç”¨æˆ·æ€»æ•°", sprintf("%d", len(users)), "total-users", "info")
    yakit.StatusCard("ç®¡ç†å‘˜æ•°é‡", sprintf("%d", adminCount), "admin-count", "warning")
    
    if validAgeCount > 0 {
