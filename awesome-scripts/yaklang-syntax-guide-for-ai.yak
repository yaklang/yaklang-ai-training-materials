processUserData = func(jsonStr) {
    // è§£æJSON
    data = json.loads(jsonStr)
    if data == nil {
        yakit.Error("JSONè§£æå¤±è´¥")
        return
    }
    
    // æå–ç”¨æˆ·æ•°ç»„
