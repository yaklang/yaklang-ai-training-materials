        log.info("   âœ“ Successfully indexed plugin: %s (entry_id: %s)", scriptName, searchResult.EntryID)
        
        result["success"] = true
        result["questions_generated"] = questionsGenerated
        result["entry_id"] = searchResult.EntryID

    } catch processErr {
